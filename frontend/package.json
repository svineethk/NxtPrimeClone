  "devDependencies": {
    "@babel/eslint-parser": "7.27.0",
    "@babel/preset-react": "7.26.3",
    "eslint": "8.2.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.1.0",
    "eslint-config-react-app": "7.0.1",
